","usedDeprecatedRules":"46"},{"filePath":"86","messages":"87","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"88","messages":"89","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"46"},{"filePath":"90","messages":"91","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/index.js",[],["92","93"],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/App.js",["94","95"],"import logo from './logo.svg';\nimport './App.css';\nimport React, { Fragment, useEffect } from 'react';\nimport Header from './components/layouts/Header';\nimport Footer from './components/layouts/Footer';\nimport Landing from './components/layouts/Landing';\nimport ItemDetails from './components/Item/ItemDetails';\nimport Cart from './components/layouts/Cart';\nimport NotFound from './components/layouts/NotFound';\nimport { BrowserRouter as Router, Route, Switch } from 'react-router-dom';\n\n// Redux\nimport { Provider } from 'react-redux';\nimport store from './store';\n\n// Items Fetch\nimport { getItems } from './actions/items';\nconst App=() => {\n  useEffect(()=>{\n    store.dispatch(getItems());\n   },[]);\n  return (\n    <Provider store={store}>\n      <Router>\n        \n       \n        <Header/>\n        <Switch>\n          <Route exact path='/' component={Landing} /> \n          <Route exact path='/cart' component={Cart} /> \n           \n          <Route exact path='/item/:id' component={ItemDetails}/>\n\n          <Route component={NotFound}/>\n        </Switch>\n        <Footer/>\n       \n      </Router>\n   </Provider>\n  );\n}\n\nexport default App;\n","/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/reportWebVitals.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/layouts/Header.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/layouts/Footer.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/layouts/Landing.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/layouts/Carousal.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/store.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/reducers/index.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/reducers/items.js",["96"],"import {\n    GET_ITEMS,\n    GET_ITEM,\n    ITEM_ERROR,\n    ADD_CART,\n    REMOVE_CART\n    \n} from '../actions/types';\nconst initialState ={\n   items:[],\n   item:null,\n   loading:true,\n   error:{}\n}\nconst addItemCount =(items,payload) =>{\n    let item=items.filter(item=>item.id===payload.id);\n    if(item){\n        item.count++;\n        return [item,...items];\n    } \n    return [payload,...items];\n\n    \n    \n}\nconst removeItemCount = (items,payload) => {\n    let item=items.filter(item=>item.id===payload);\n    if(item.count>1){\n        item.count--;\n        return [item,...items];\n    }\n    return items.filter(item=>item.id!==payload);\n}\nexport default function(state=initialState,action){\n    const { type, payload } =action;\n    switch(type){\n        \n        case GET_ITEMS:\n            return {\n             ...state,\n             items:payload,\n             loading:false\n            }  \n        case GET_ITEM:\n            return {\n                 ...state,\n                 item:payload,\n                 loading:false\n            }\n    \n        case ADD_CART:\n            return {\n                    ...state,\n                    cart:addItemCount(state.items,payload),\n                    loading:false   \n            }\n        case REMOVE_CART:\n            return {\n                    ...state,\n                    cart:removeItemCount(state.items,payload),\n                    loading:false\n            }\n        case ITEM_ERROR:\n            return {\n                ...state,\n                error:payload,\n                loading:false\n            }\n        \n     \n        default: return state\n    }\n\n}","/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/actions/types.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/actions/items.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/Item/ListItem.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/Item/ItemDetails.js",["97"],"import React,{ useEffect,useState } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport PropTypes from 'prop-types';\nimport { getItem } from '../../actions/items';\nimport { connect } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport Spinner from '../layouts/Spinner';\nimport AddItemModal from '../Utilities/Modals/AddItemModal';\nimport { addItemToCart } from '../../actions/cart';\n\nconst ItemDetails = ({match,getItem,items:{item,error,loading},addItemToCart})=> {\n    const [addButtonState,setAddButtonState]=useState(false);\n    const history=useHistory();\n\n    const addToCart = (addItem) =>{\n        setAddButtonState(!addButtonState);\n        addItemToCart(addItem._id);\n    }\n    let flag=0;\n        \n        \n    if(window.scrollTop > 0){\n        window.scrollTo(0,0);\n    }\n       \n \n    \n    useEffect(()=>{\n        getItem(match.params.id);\n       \n    })\n    \n    if (item!==null && loading===false)  {\n       \n        return (\n        <section>\n            { addButtonState && <AddItemModal name={item.name} id={item._id} setState={setAddButtonState}  state={addButtonState}/> }\n            <div className=\"container py-5 fade-in\">\n                <div className=\"container p-4 border rounded-lg shadow\">\n                    <div className=\"d-inline-block pb-3 pointer d-\">\n                    <Link to='/' className=\"btn\" style={{textDecoration:'none'}}><i className=\"fa fa-arrow-left pr-2\"></i>Back to Catalog</Link>\n                    </div>\n                    <div className=\"row pb-3\">\n                        <div className=\"col-lg-5 px-3 pb-3 text-center\">\n                            <img src={process.env.PUBLIC_URL + `/img/${item.image}`} alt={item.image} className=\"fit\"/>\n                        </div>\n                    <div className=\"col-lg-7 pb-3 d-flex align-items-center\">\n                    <div className=\"details-text\"><h5>{item.name}</h5><h6 className=\"text-muted\">Rs. {item.price}</h6>\n                            <button type=\"button\" className=\"btn btn-primary\"  onClick={() => addToCart(item)}>Add to Cart</button>\n                        </div>\n                    </div>\n                    </div>\n                <div className=\"long-description\">\n                    <h5>Description</h5>\n    <p>{item.description}</p>\n                </div>\n            </div>\n        </div>\n\n</section>\n    ) }\n    else if(error.status!==400 || loading!==false) {\n            return <Spinner/>;\n    }  else {\n            \n          history.push('/pagenotfound');\n          return <Spinner/>;\n       \n    }\n    \n}\n\nItemDetails.propTypes = {\n  getItems:PropTypes.func.isRequired,\n  items:PropTypes.object.isRequired\n}\nconst mapStateToProps= state =>({\n    items:state.items\n});\nexport default connect(mapStateToProps,{getItem,addItemToCart})(ItemDetails)\n","/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/layouts/Spinner.js",["98"],"import React, { Fragment } from 'react';\nimport spinner from './spinner.gif';\n\nexport default () => (\n  <Fragment>\n    <div style={{width:'100vh',height:'100%',padding:'10%',margin:'0 auto'}}>\n      <img\n        src={spinner}\n        style={{ width: '200px', margin: 'auto', display: 'block' }}\n        alt='Loading...'\n      />\n    </div>\n  </Fragment>\n);","/home/shreyas/Projects/Nod