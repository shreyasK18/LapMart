state,\n                 item:payload,\n                 loading:false\n            }\n    \n        case ADD_CART:\n            return {\n                    ...state,\n                    cart:addItemCount(state.items,payload),\n                    loading:false   \n            }\n        case REMOVE_CART:\n            return {\n                    ...state,\n                    cart:removeItemCount(state.items,payload),\n                    loading:false\n            }\n        case ITEM_ERROR:\n            return {\n                ...state,\n                error:payload,\n                loading:false\n            }\n        \n     \n        default: return state\n    }\n\n}",["117","118"],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/actions/types.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/actions/items.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/Item/ListItem.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/Item/ItemDetails.js",["119"],"import React,{ useEffect,useState } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport PropTypes from 'prop-types';\nimport { getItem } from '../../actions/items';\nimport { connect } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport Spinner from '../layouts/Spinner';\nimport AddItemModal from '../Utilities/Modals/AddItemModal';\nimport { addItemToCart,addCart, getCart } from '../../actions/cart';\nimport currencyFormatter  from '../../utilities/currencyFormatter';\n\nconst ItemDetails = ({match,getItem,items:{item,error,loading},addItemToCart,addCart,cart:{_id},getCart})=> {\n    const [addButtonState,setAddButtonState]=useState(false);\n    const [scrollUp,setScrollUp]=useState(false);\n    const history=useHistory();\n\n    const addToCart = () =>{\n        setAddButtonState(!addButtonState);\n        if(_id!==null && _id!==undefined){\n            addItemToCart(_id,item._id,item.price);\n        } else {\n            addCart(item._id,item.price);\n        }\n       getCart(_id);\n    }\n    \n        \n        \n    \n    useEffect(()=>{\n        getItem(match.params.id);\n    },[item])\n  \n    if (item!==null && loading===false)  {\n        if(scrollUp===false){\n            window.scrollTo(0,0);\n            setScrollUp(true);\n        }\n        \n        return (\n        <section>\n            { addButtonState && <AddItemModal name={item.name} id={item._id} setState={setAddButtonState}  state={addButtonState}/> }\n            <div className=\"container py-5 fade-in\">\n                <div className=\"container p-4 border rounded-lg shadow\">\n                    <div className=\"d-inline-block pb-3 pointer d-\">\n                         <Link to='/' className=\"btn\" style={{textDecoration:'none'}}><i className=\"fa fa-arrow-left pr-2\"></i>Back to Store</Link>\n                    </div>\n                    <div className=\"row pb-3\">\n                        <div className=\"col-xl-5 col-lg-12 px-3 pb-3 text-center\">\n                            <img src={process.env.PUBLIC_URL + `/img/${item.image}`} alt={item.image} className=\"fit\"/>\n                        </div>\n                        <div className=\"col-lg-7 pb-3 d-flex align-items-center\">\n                            <div className=\"details-text\"><h5>{item.name}</h5><h6 className=\"text-muted\">{currencyFormatter(item.price)}</h6>\n                                <button type=\"button\" className=\"btn btn-primary\"  onClick={() => addToCart()}>Add to Cart</button>\n                            </div>\n                        </div>\n                    </div>\n                    <div className=\"long-description\">\n                        <h5>Description</h5>\n                        <p>{item.description}</p>\n                    </div>\n                </div>\n            </div>\n        </section>\n    ) }\n    else if(error.status!==400 || loading!==false) {\n            return <Spinner/>;\n    }  else {\n            \n          history.push('/pagenotfound');\n          return <Spinner/>;\n       \n    }\n    \n}\n\nItemDetails.propTypes = {\n  addItemToCart:PropTypes.func.isRequired,\n  addCart:PropTypes.func.isRequired,\n  getItems:PropTypes.func.isRequired,\n  items:PropTypes.object.isRequired,\n  currencyFormatter:PropTypes.func.isRequired,\n}\nconst mapStateToProps= state =>({\n    items:state.items,\n    cart:state.cart\n});\nexport default connect(mapStateToProps,{getItem,addItemToCart,addCart,getCart})(ItemDetails)\n","/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/layouts/Spinner.js",["120"],"import React, { Fragment } from 'react';\nimport spinner from './spinner.gif';\n\nexport default () => (\n  <Fragment>\n    <div style={{width:'100vh',height:'100%',padding:'10%',margin:'0 auto'}}>\n      <img\n        src={spinner}\n        style={{ width: '200px', margin: 'auto', display: 'block' }}\n        alt='Loading...'\n      />\n    </div>\n  </Fragment>\n);","/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/Utilities/Modals/AddItemModal.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/components/layouts/NotFound.js",[],"/home/shreyas/Projects/Nodejs/Actual Project/LapMart/client/src/reducers/cart.js",["121"],"import {\n    GET_CART,\n    ADD_CART,\n    ADD_ITEM_TO_CART,\n    REMOVE_ITEM_FROM_CART,\n    REMOVE_CART,\n    CART_ERROR,\n    CART_EMPTY,\n    ORDER_CART\n    \n} from '../actions/types';\nconst initialState ={\n   items:null,\n   count:0,\n   totalPrice:0,\n   loading:true,\n   _id:null,\n   itemdetails:null,\n   ordered:false,\n   error:{}\n}\n\n\n\nexport default function(state=initialState,action){\n    const { type, payload } =action;\n    switch(type){\n        case GET_CART:\n            return {\n                ...state,\n                items:payload.items,\n                count:payload.count,\n      